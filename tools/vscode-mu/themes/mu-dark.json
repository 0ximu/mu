{
  "name": "MU Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#c0caf5",
    "editorLineNumber.foreground": "#3b4261",
    "editorLineNumber.activeForeground": "#737aa2"
  },
  "tokenColors": [
    {
      "name": "Comments & Metadata",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#565f89",
        "fontStyle": "italic"
      }
    },
    {
      "name": "MU Version",
      "scope": "constant.numeric.version.mu",
      "settings": {
        "foreground": "#ff9e64",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Section Headers",
      "scope": [
        "markup.heading.section.mu",
        "entity.name.section.mu"
      ],
      "settings": {
        "foreground": "#7dcfff",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Module Sigil (!)",
      "scope": [
        "keyword.operator.sigil.module.mu",
        "keyword.operator.sigil.mu"
      ],
      "settings": {
        "foreground": "#ff007c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Module Names",
      "scope": "entity.name.namespace.mu",
      "settings": {
        "foreground": "#bb9af7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Module Keyword",
      "scope": "keyword.declaration.module.mu",
      "settings": {
        "foreground": "#ff007c"
      }
    },
    {
      "name": "Class Sigil ($)",
      "scope": "keyword.operator.sigil.class.mu",
      "settings": {
        "foreground": "#2ac3de",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class Names",
      "scope": "entity.name.class.mu",
      "settings": {
        "foreground": "#2ac3de",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class.mu",
      "settings": {
        "foreground": "#7aa2f7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Sigil (#)",
      "scope": "keyword.operator.sigil.function.mu",
      "settings": {
        "foreground": "#9ece6a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Names",
      "scope": [
        "entity.name.function.mu",
        "entity.name.function.action.mu"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Async Modifier",
      "scope": "storage.modifier.async.mu",
      "settings": {
        "foreground": "#bb9af7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter.mu",
      "settings": {
        "foreground": "#e0af68",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types - Generic",
      "scope": "support.type.generic.mu",
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Types - Primitive",
      "scope": "support.type.primitive.mu",
      "settings": {
        "foreground": "#2ac3de"
      }
    },
    {
      "name": "Types - Custom",
      "scope": "entity.name.type.mu",
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Return Arrow",
      "scope": [
        "keyword.operator.return-type.mu",
        "keyword.operator.arrow.mu"
      ],
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "name": "Decorator @ Symbol",
      "scope": "punctuation.definition.decorator.mu",
      "settings": {
        "foreground": "#f7768e"
      }
    },
    {
      "name": "Decorator Names (attrs, deps)",
      "scope": [
        "keyword.other.attrs.mu",
        "keyword.other.deps.mu",
        "entity.name.decorator.mu"
      ],
      "settings": {
        "foreground": "#f7768e"
      }
    },
    {
      "name": "Attribute Names",
      "scope": "variable.other.property.mu",
      "settings": {
        "foreground": "#73daca"
      }
    },
    {
      "name": "Import Names",
      "scope": "entity.name.import.mu",
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Annotation Sigil (::)",
      "scope": "keyword.operator.annotation.mu",
      "settings": {
        "foreground": "#ff9e64",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Annotation Keywords",
      "scope": [
        "keyword.control.guard.mu",
        "keyword.control.transaction.mu",
        "keyword.control.external.mu",
        "keyword.other.annotation-key.mu"
      ],
      "settings": {
        "foreground": "#e0af68",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Annotation Values",
      "scope": [
        "string.unquoted.guard-condition.mu",
        "string.unquoted.annotation-value.mu"
      ],
      "settings": {
        "foreground": "#9ece6a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Transaction Type",
      "scope": "constant.language.transaction-type.mu",
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "External API Calls",
      "scope": "entity.name.function.external.mu",
      "settings": {
        "foreground": "#7dcfff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Conditional (?)",
      "scope": "keyword.control.conditional.mu",
      "settings": {
        "foreground": "#f7768e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Condition Variable",
      "scope": "variable.other.condition.mu",
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Step Numbers",
      "scope": "constant.numeric.step-number.mu",
      "settings": {
        "foreground": "#ff9e64",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Step Descriptions",
      "scope": "string.unquoted.step-description.mu",
      "settings": {
        "foreground": "#a9b1d6"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double.mu",
        "string.quoted.single.mu"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Constants - Enum Members",
      "scope": "constant.other.enum-member.mu",
      "settings": {
        "foreground": "#ff9e64",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constants - Boolean",
      "scope": "constant.language.boolean.mu",
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Constants - Numeric",
      "scope": [
        "constant.numeric.mu",
        "constant.numeric.complexity.mu"
      ],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Mutation Operator (=>)",
      "scope": "keyword.operator.mutation.mu",
      "settings": {
        "foreground": "#f7768e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import Operator",
      "scope": "keyword.control.import.mu",
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Inheritance Operator (<)",
      "scope": "keyword.operator.inheritance.mu",
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "name": "Punctuation - Brackets",
      "scope": [
        "punctuation.definition.array.begin.mu",
        "punctuation.definition.array.end.mu",
        "punctuation.definition.parameters.begin.mu",
        "punctuation.definition.parameters.end.mu",
        "punctuation.definition.generic.begin.mu",
        "punctuation.definition.generic.end.mu"
      ],
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "name": "Punctuation - Separators",
      "scope": [
        "punctuation.separator.comma.mu",
        "punctuation.separator.colon.mu"
      ],
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "name": "Metadata Keys",
      "scope": "keyword.other.metadata-key.mu",
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "Metadata Values",
      "scope": "string.unquoted.metadata-value.mu",
      "settings": {
        "foreground": "#73daca"
      }
    },
    {
      "name": "Note Text",
      "scope": "comment.line.note-text.mu",
      "settings": {
        "foreground": "#e0af68",
        "fontStyle": "italic"
      }
    }
  ]
}
